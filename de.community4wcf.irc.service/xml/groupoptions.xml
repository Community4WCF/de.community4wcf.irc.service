<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com/XSD/groupoptions.xsd">
	<import>
		<categories>

			<category name="admin.ircservice">
				<parent>admin</parent>
			</category>

			<category name="user.ircservice">
				<parent>user</parent>
			</category>

			<category name="admin.ircservice.eggdrop">
				<parent>admin.ircservice</parent>
				<showorder>1</showorder>
			</category>
		</categories>
		<options>
			<!-- Admin -->
			<option name="admin.ircservice.canAddEggdrop">
				<categoryname>admin.ircservice.eggdrop</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>1</showorder>
			</option>
			<option name="admin.ircservice.canEditEggdrop">
				<categoryname>admin.ircservice.eggdrop</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>2</showorder>
			</option>
			<option name="admin.ircservice.canDeleteEggdrop">
				<categoryname>admin.ircservice.eggdrop</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>3</showorder>
			</option>
			
			<!-- User -->
			<option name="user.ircservice.canView">
				<categoryname>user.ircservice</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>1</showorder>
			</option>
			<option name="user.ircservice.canOrder">
				<categoryname>user.ircservice</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>2</showorder>
			</option>
		</options>
	</import>
</data>